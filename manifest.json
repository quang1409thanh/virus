{
    "manifest_version": 3,
    "name": "My Cool Extension",
    "version": "0.1",
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_icon": "icon.png"
    },
    "content_scripts": [
      {
        "matches": [
          "<all_urls>"
        ],
        "js": ["content.js"]
      }
    ]
}
